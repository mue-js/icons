{
    "name": "@mue-js/icons",
    "version": "1.0.3",
    "description": "A not professional React package. It'll help you to easily make grids and recurrent components, for sure.",
    "main": "src/index.ts",
    "exports": {
        ".": "./src/index.ts",
        "./": "./src/lib/",
        "./raw": "./src/components/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mue-js/icons.git"
    },
    "bugs": {
        "url": "https://github.com/mue-js/icons/issues"
